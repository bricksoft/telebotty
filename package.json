{
    "name": "telebot",
    "version": "1.2.0",
    "description": "telegram bot which listens to several requests of seedbox",
    "main": "server.js",
    "scripts": {
        "test": "./node_modules/.bin/mocha",
        "test-coverage": "NODE_ENV=test YOURPACKAGE_COVERAGE=1 ./node_modules/.bin/mocha --require blanket --reporter mocha-lcov-reporter | ./node_modules/coveralls/bin/coveralls.js"
    },
    "author": "bricksoft",
    "license": "GPL3",
    "private": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/bricksoft/telebotty-node"
    },
    "dependencies": {
        "assert": "^1.4.1",
        "body-parser": "~1.16.0",
        "cookie-parser": "~1.4.3",
        "express": "^4.14.1",
        "fs": "0.0.1-security",
        "hbs": "~4.0.1",
        "irc": "^0.5.2",
        "material-components-web": "^0.7.0",
        "material-design-lite": "^1.3.0",
        "mongoose": "^4.8.1",
        "node-sass-middleware": "0.9.8",
        "serve-favicon": "~2.3.2",
        "socket.io": "^1.7.2",
        "tgfancy": "^0.8.0"
    },
    "devDependencies": {
        "blanket": "^1.2.3",
        "chai": "^3.5.0",
        "chai-http": "^3.0.0",
        "coveralls": "^2.13.0",
        "mocha": "^3.2.0",
        "mocha-lcov-reporter": "^1.3.0",
        "mocha-logger": "^1.0.5",
        "nodemon": "^1.11.0",
        "opbeat": "^4.13.0"
    },
    "blanket": {
        "pattern": [
            ""
        ],
        "data-cover-never": [
            ".vscode",
            "cloc",
            "node_modules",
            "test",
            "tests"
        ]
    },
    "jshintConfig": {
        "esversion": 6
    },
    "eslintConfig": {
        "parserOptions": {
            "ecmaVersion": 6
        }
    }
}