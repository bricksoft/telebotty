// Platzieren Sie Ihre Einstellungen in dieser Datei, um Standard- und Benutzereinstellungen zu Ã¼berschreiben.
{
    "debug.inlineValues": true,
    "docthis.automaticForBlockComments": true,
    "editor.formatOnSave": true,
    "files.autoSave": "off",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "old/**/*": true
    },
    "jshint.enable": true,
    "eslint.enable": true,
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "todoIgnore": [
        "cloc/**",
        "**/node_modules/**"
    ]
}